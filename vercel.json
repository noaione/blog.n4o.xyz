{
    "github": {
        "silent": true
    },
    "cleanUrls": true,
    "headers": [{
        "source": "/(.*)",
        "headers": [{
                "key": "X-Git-Source",
                "value": "https://github.com/noaione/blog.n4o.xyz"
            },
            {
                "key": "X-Made-With",
                "value": "NextJS and TailwindCSS <3"
            },
            {
                "key": "Permissions-Policy",
                "value": "interest-cohort=()"
            }
        ]
    }]
}