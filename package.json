{
  "name": "tailwind-nextjs-starter-blog",
  "version": "1.1.0-noaione",
  "private": true,
  "scripts": {
    "start": "node ./scripts/next-watch.js ./data",
    "dev": "next dev -p 4500",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev -p 4500",
    "build": "next build && node ./scripts/generate-sitemap && node ./scripts/generate-rss.js",
    "serve": "next start",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint --ext ts --ext tsx pages/ lib/ components/ layouts/ data/",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "dependencies": {
    "@badrap/bar-of-progress": "0.1.2",
    "@heroicons/react": "1.0.5",
    "@kachkaev/react-inline-js": "0.2.0",
    "gray-matter": "4.0.3",
    "image-size": "1.0.0",
    "luxon": "2.1.1",
    "mdx-bundler": "8.0.0",
    "next": "12.0.5-canary.10",
    "preact": "10.6.1",
    "preact-render-to-string": "5.1.19",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.22.0",
    "react-medium-image-zoom": "4.3.5",
    "reading-time": "1.5.0",
    "rehype": "12.0.0",
    "rehype-highlight": "^5.0.0",
    "rehype-katex": "6.0.2",
    "rehype-parse": "^8.0.3",
    "rehype-prism-plus": "1.1.3",
    "rehype-stringify": "^9.0.2",
    "rehype-twemojify": "^3.1.0",
    "remark": "14.0.2",
    "remark-autolink-headings": "7.0.1",
    "remark-breaks": "3.0.2",
    "remark-footnotes": "4.0.1",
    "remark-frontmatter": "4.0.1",
    "remark-gemoji": "7.0.1",
    "remark-gfm": "3.0.1",
    "remark-highlight.js": "7.0.1",
    "remark-html": "15.0.0",
    "remark-math": "5.1.1",
    "remark-parse": "10.0.1",
    "remark-slug": "7.0.1",
    "tiny-glob": "0.2.9",
    "trim": "1.0.1",
    "twemoji": "13.1.0",
    "video.js": "7.17.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.0.1",
    "@svgr/webpack": "6.0.0",
    "@tailwindcss/forms": "0.3.4",
    "@tailwindcss/typography": "0.4.1",
    "@types/express": "^4.17.13",
    "@types/luxon": "2.0.7",
    "@types/node": "16.11.10",
    "@types/react": "17.0.37",
    "@types/twemoji": "12.1.2",
    "@types/video.js": "7.3.27",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "autoprefixer": "10.4.0",
    "cross-env": "7.0.3",
    "esbuild": "0.14.0",
    "eslint": "8.3.0",
    "eslint-config-next": "12.0.5-canary.10",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "file-loader": "6.2.0",
    "globby": "12.0.2",
    "postcss": "8.4.4",
    "prettier": "2.5.0",
    "tailwindcss": "2.2.19",
    "typescript": "4.5.2",
    "unified": "10.1.1",
    "unist-util-visit": "4.1.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
