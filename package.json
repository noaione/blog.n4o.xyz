    "asciinema-player": "3.0.1",
    "axios": "0.27.2",
    "emoji-regex": "10.1.0",
    "gray-matter": "4.0.3",
    "image-size": "1.0.2",
    "luxon": "3.0.1",
    "mdx-bundler": "9.0.1",
    "next": "12.2.5",
    "preact": "10.10.6",
    "preact-render-to-string": "5.2.2",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.0.5",
    "react-medium-image-zoom": "5.0.1",
    "react-multi-carousel": "^2.8.2",
    "reading-time": "1.5.0",
    "rehype": "12.0.1",
    "rehype-highlight": "5.0.2",
    "rehype-katex": "6.0.2",
    "rehype-parse": "8.0.4",
    "rehype-prism-plus": "1.4.2",
    "rehype-slug": "5.0.1",
    "rehype-stringify": "9.0.3",
    "remark": "14.0.2",
    "remark-autolink-headings": "7.0.1",
    "remark-breaks": "3.0.2",
    "remark-footnotes": "4.0.1",
    "remark-frontmatter": "4.0.1",
    "remark-gemoji": "7.0.1",
    "remark-gfm": "3.0.1",
    "remark-html": "15.0.1",
    "remark-math": "5.1.1",
    "remark-parse": "10.0.1",
    "tiny-glob": "0.2.9",
    "trim": "1.0.1",
    "twemoji": "14.0.2",
    "unist-util-map": "3.1.1"
