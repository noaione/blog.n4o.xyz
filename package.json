{
  "name": "tailwind-nextjs-starter-blog",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "start": "cross-env next-remote-watch ./data",
    "dev": "next dev -p 4500",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev -p 4500",
    "build": "next build && node ./scripts/generate-sitemap && node ./scripts/generate-rss.js",
    "serve": "next start",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint --ext ts --ext tsx pages/ lib/ components/ layouts/ data/",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "dependencies": {
    "@badrap/bar-of-progress": "^0.1.2",
    "@heroicons/react": "^1.0.1",
    "@kachkaev/react-inline-js": "^0.2.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.2.5",
    "gray-matter": "^4.0.2",
    "image-size": "^1.0.0",
    "luxon": "^1.27.0",
    "mdx-bundler": "^5.1.2",
    "next": "^11.0.1",
    "next-compose-plugins": "^2.2.1",
    "next-seo": "^4.26.0",
    "next-themes": "^0.0.14",
    "next-transpile-modules": "^8.0.0",
    "postcss": "^8.3.5",
    "preact": "^10.5.13",
    "preact-render-to-string": "^5.1.19",
    "prop-types": "^15.7.2",
    "querystring": "^0.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.17.4",
    "react-medium-image-zoom": "4.3.3",
    "reading-time": "1.3.0",
    "rehype-katex": "^5.0.0",
    "rehype-prism-plus": "^0.0.2",
    "remark": "^13.0.0",
    "remark-admonitions": "^1.2.1",
    "remark-autolink-headings": "6.0.1",
    "remark-breaks": "^2.0.2",
    "remark-footnotes": "2.0.0",
    "remark-gemoji": "^6.0.0",
    "remark-gfm": "^1.0.0",
    "remark-highlight.js": "^6.0.0",
    "remark-html": "^13.0.1",
    "remark-math": "^4.0.0",
    "remark-mdx": "^2.0.0-next.9",
    "remark-slug": "6.0.0",
    "tailwindcss": "^2.2.2",
    "tiny-glob": "^0.2.9",
    "trim": "^1.0.1",
    "twemoji": "^13.1.0",
    "video.js": "^7.12.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "10.2.3",
    "@svgr/webpack": "^5.5.0",
    "@types/luxon": "^1.27.0",
    "@types/node": "^15.14.0",
    "@types/react": "^17.0.13",
    "@types/twemoji": "^12.1.1",
    "@types/video.js": "^7.3.22",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.12.16",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "file-loader": "^6.0.0",
    "globby": "^11.0.3",
    "prettier": "2.2.1",
    "rehype": "11.0.0",
    "remark-frontmatter": "3.0.0",
    "remark-parse": "^9.0.0",
    "remark-stringify": "^9.0.1",
    "typescript": "^4.3.5",
    "unified": "9.2.1",
    "unist-util-visit": "^3.1.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
